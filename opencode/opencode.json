 {
     "theme": "ayu",
     "$schema": "https://opencode.ai/config.json",
    "keybinds": {
        "leader": "ctrl+o",
        "app_exit": "ctrl+q",
        "editor_open": "ctrl+e",
        "theme_list": "alt+t",
        "session_new": "<leader>n",
        "session_list": "ctrl+s",
        "session_interrupt": "esc",
        "session_child_cycle": "ctrl+right",
        "session_child_cycle_reverse": "ctrl+left",
        "model_list": "alt+m",
        "agent_list": "alt+a",
        "command_list": "ctrl+p",
        "input_paste": "ctrl+v",
        "messages_copy": "<leader>y",
        "messages_half_page_down": "alt+j",
        "messages_half_page_up": "alt+k",
        "history_previous": "ctrl+p",
        "history_next": "ctrl+n",
    }
}
